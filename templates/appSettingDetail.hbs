<div id="appSettingDetail">

 {{#if hasControl}}
  <div class="bulkUserActions app-setting-rbs">
    <div class="setting-heading">
        <label class="bulk">{{getString 'bulkUserActions'}}</label>
    </div>
    <ul class="list-group">

      <li class="lists-cont d-flex justify-content-between align-items-center" id ="contrAudioAll">
          <div class="appSettingIcons" id="usersMuteSwitch"></div>
        {{getString 'muteAllAudio'}}


        <label class="switch">
          <input type="checkbox" >
          <span class="slider round icon-all-audio-enable congtooltip" id ="contrAudioAllImg" ></span>
        </label>

      </li>

      <li class="lists-cont d-flex justify-content-between align-items-center" id ="contrVideoAll">
        <div class="appSettingIcons" id="usersVideoSwitch"></div>
        {{getString 'disableAllVideo'}}
        <label class="switch video_toggle">
          <input type="checkbox">
          <span class="slider round icon-all-video-enable congtooltip" id ="contrVideoAllImg" ></span>
        </label>
      </li>
   </ul>
    </div> 
    

{{/if}}
 <div id="settingGeneral" class="app-settings-rbs">
     <div class="setting-heading">
      <label>{{getString 'general'}}</label>
     </div>
        <ul class="list-group">
            <li class="lists-cont d-flex justify-content-between align-items-center">
            <div class="appSettingIcons" id="validate-precheck"></div>
            {{getString 'appPrerequites'}}
            <span class="pre-check-btn"><div id="settingPrecheck">
              <div class="prechk" data-title="{{getString 'precheck'}}" id="precheckTest">
                <span class="precheck cgIcon"  id="precheckSetting" >{{getString 'precheckStart'}}</span>
              </div>
        </div></span>
            </li>
        </ul>
    
  </div>  
    
  <div id="settingAudioVideo" style="display:none;">
    {{#if hasControl}}
      <div class="uiMuteAll">
        <a id="contrAudioAll">
            <span class="cgIcon"id="contrAudioAllImg" data-action="disable"
                  class="icon-all-audio-disable cgIcon"
                  data-title="{{getString 'muteAll'}}">
            </span>
        </a>
      </div>
    {{/if}}
  </div>

  
   <div id="settingMedia" class="app-settings-rbs">
    <div id="webRtcIo" class="io" data-suggestion="" >
<!--
      <a id="ancIo">
        <span id="wbRcio" class="rtc  congtooltip"  data-title="webrtc io" >settingMedia </span>
      </a>
-->
    </div>
  </div>
  
</div>
