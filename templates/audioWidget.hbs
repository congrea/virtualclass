      <a id="speakerPressonceAnch" class="congtooltip" data-title="{{audio_tooltip}}" name="speakerPressonceAnch">
        <span id="speakerPressonceLabel" class="silenceDetect" data-silence-detect="stop"> <i> </i> </span>
      </a>
    </li>

    <li class="prechk congtooltip" data-title="Precheck" id="precheckTest">
      <a><span class="precheck "  id="precheckSetting" ></span></a>
