      <div id="timerCont"></div>
    </div>
    <div id="votesWrapper" class="modal-header">
    </div>
  </div>
  <div id="resultLayoutBody" class="container stdlayout">
    <label>{{getString 'Question'}} : <span id="qnLabelCont">{{obj.question}}</span></label>

    <div  class="panel">
      <div id="optnNonVotd">

      </div>
    </div>

    <div id="chartMenuCont" class="panel">
      <button id="bar" class="btn btn-default">
